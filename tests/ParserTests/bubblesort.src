func bubbleSort(arr: int[], size: int) -> void 
{
  let n: int;
  let i: int;
  let j: int;
  let temp: int; 
  n = size;
  i = 0;
  j = 0;
  temp = 0;
  while (i < n-1) { 
    while (j < n-i+1) {
      if (j > j+1) 
        then {
          temp = j;
          j = j+1;
          
        } else ;
        j = j+1;
      };
    i = i+1;
  };
}
   
func printArray(arr: int[], size: int) -> void 
{
  let n: int;
  let i: int; 
  n = size;
  i = 0; 
  while (i<n) { 
    write(arr[i]);
      i = i+1;
  };
} 

